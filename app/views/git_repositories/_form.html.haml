= form_for @git_repository do |f|
  -if @git_repository.errors.any?
    #error_explanation
      %h2= "#{pluralize(@git_repository.errors.count, "error")} prohibited this git_repository from being saved:"
      %ul
        - @git_repository.errors.full_messages.each do |msg|
          %li= msg

  .actions
    = f.submit 'Save'
